<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#8EC5F4"
    tools:context=".screens.MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="339dp"
        android:layout_height="275dp"
        android:gravity="center"
        android:text="Eat \n &amp; \n Share"
        android:textSize="70dp"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="84dp" />

    <Button
        android:id="@+id/btnLogin1"
        android:layout_width="111dp"
        android:layout_height="51dp"
        android:backgroundTint="#7EAEF6"
        android:onClick=""
        android:text="התחברות"
        tools:ignore="OnClick"
        tools:layout_editor_absoluteX="77dp"
        tools:layout_editor_absoluteY="623dp" />

    <Button
        android:id="@+id/btnreg1"
        android:layout_width="115dp"
        android:layout_height="46dp"
        android:backgroundTint="#7EAEF6"
        android:text="הרשמה"
        tools:layout_editor_absoluteX="204dp"
        tools:layout_editor_absoluteY="624dp" />

    <Button
        android:id="@+id/Odot1"
        android:layout_width="114dp"
        android:layout_height="41dp"
        android:backgroundTint="#7EAEF6"
        android:text="אודות"
        tools:layout_editor_absoluteX="138dp"
        tools:layout_editor_absoluteY="674dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="343dp"
        android:layout_height="186dp"
        app:srcCompat="@drawable/pic11"
        
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="359dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="126dp"
        android:layout_height="137dp"
        app:srcCompat="@drawable/pic22"
        tools:layout_editor_absoluteX="114dp"
        tools:layout_editor_absoluteY="7dp" />

</androidx.constraintlayout.widget.ConstraintLayout>